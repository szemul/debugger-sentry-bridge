{
    "name": "szemul/debugger-sentry-bridge",
    "description": "This package allows enriching Sentry errors and traces from the debug information collected by the szemul/debugger package",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0.0",
        "sentry/sentry": "^3.3.2",
        "szemul/debugger": "^1.0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "guzzlehttp/psr7": "^2.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.4.0",
        "phpunit/phpunit": "^9.5.5",
        "sentry/sdk": "^3.1.0",
        "szemul/database": "^1.0.0|^2.0.0|^3.0.0"
    },
    "suggest": {
        "szemul/database": "For capturing database events"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\DebuggerSentryBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\DebuggerSentryBridge\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
